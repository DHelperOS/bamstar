{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to MCP (Node)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": false,
      "localRoot": "${workspaceFolder}/mcp/supabase-mcp",
      "remoteRoot": "${workspaceFolder}/mcp/supabase-mcp"
    },
    {
      "name": "Attach to context7-mcp",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "localRoot": "${workspaceFolder}/mcp/context7-mcp",
      "remoteRoot": "${workspaceFolder}/mcp/context7-mcp"
    },
    {
      "name": "Attach to serena-mcp",
      "type": "node",
      "request": "attach",
      "port": 9230,
      "localRoot": "${workspaceFolder}/mcp/serena-mcp",
      "remoteRoot": "${workspaceFolder}/mcp/serena-mcp"
    },
    {
      "name": "Attach to sequential-mcp",
      "type": "node",
      "request": "attach",
      "port": 9235,
      "localRoot": "${workspaceFolder}/mcp/sequential-mcp",
      "remoteRoot": "${workspaceFolder}/mcp/sequential-mcp"
    },
    {
      "name": "Flutter: Launch bamstar (lib/main.dart)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Flutter: Attach to Device",
      "request": "attach",
      "type": "dart"
    }
  ]
}
